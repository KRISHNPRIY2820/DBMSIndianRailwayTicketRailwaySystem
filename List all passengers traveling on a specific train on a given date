SELECT 
    p.PAX_Name,
    p.Class_code,
    p.Seat_no,
    p.Booking_status,
    tr.From_station,
    tr.To_station
FROM 
    PAX_info p
JOIN 
    Ticket_Reservation tr ON p.PNR_no = tr.PNR_no
WHERE 
    tr.Train_code = '12302'
    AND tr.From_date = '2023-06-03'
    AND p.Booking_status IN ('Confirmed', 'RAC')
ORDER BY 
    p.Class_code, p.Seat_no;